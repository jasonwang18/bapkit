<?xml version="1.0" encoding="utf-8"?>
<com.supcon.common.view.ptr.PtrFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:core_ptr="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bgGray2"
    android:id="@+id/refreshFrameLayout"
    core_ptr:ptr_keep_header_when_refresh="true"
    core_ptr:ptr_pull_to_fresh="false"
    core_ptr:ptr_ratio_of_header_height_to_refresh="0.5"
    core_ptr:ptr_resistance="0.5">
    <com.supcon.mes.mbap.view.CustomScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:id="@+id/contentView"
            >
            <com.supcon.mes.mbap.view.CustomAdView
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:id="@+id/customAdView"
                />
        </LinearLayout>

    </com.supcon.mes.mbap.view.CustomScrollView>


</com.supcon.common.view.ptr.PtrFrameLayout>
